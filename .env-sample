# User and Group IDs
PUID=1000
PGID=1000

# OpenVPN Configuration
OPENVPN_PROVIDER=your_provider
OPENVPN_USERNAME=your_username
OPENVPN_PASSWORD=your_password
OPENVPN_CONFIG=your_config_file
LOCAL_NETWORK=192.168.1.0/24

# Transmission VPN & Proxy
WEBPROXY_ENABLED=true
WEBPROXY_PORT=8080
WEBPROXY_USERNAME=proxy_user
WEBPROXY_PASSWORD=proxy_pass
TRANSMISSION_DOWNLOAD_DIR=/downloads/completed
TRANSMISSION_INCOMPLETE_DIR=/downloads/incomplete
PEER_DNS=auto
PEER_DNS_PIN_ROUTES=true

# Network Settings
SUBNET=192.168.2.0/24
GATEWAY=192.168.2.1

# Port Configuration
TORRENT_WEBUI_PORT=9091
SONARR_PORT=8989
RADARR_PORT=7878
LIDARR_PORT=8686
PROWLARR_PORT=9696
FLARESOLVERR_PORT=8191
PLEX_PORT=32400
OVERSEERR_PORT=5055
CALIBRE_GUI_PORT=8080
CALIBRE_SERVER_PORT=8081
CALIBRE_WEB_PORT=8083
READARR_PORT=8787

# Volume Paths - Update to match actual paths
DOWNLOADS_PATH=/path/to/downloads
TORRENT_CONFIG_PATH=/path/to/torrent/config
SERIES_PATH=/path/to/series
MOVIES_PATH=/path/to/movies
MUSIC_PATH=/path/to/music
SONARR_CONFIG_PATH=/path/to/sonarr/config
RADARR_CONFIG_PATH=/path/to/radarr/config
LIDARR_CONFIG_PATH=/path/to/lidarr/config
PROWLARR_CONFIG_PATH=/path/to/prowlarr/config
PLEX_CONFIG_PATH=/path/to/plex/config
TRANSCODE_PATH=/path/to/plex/transcode
OVERSEERR_CONFIG_PATH=/path/to/overseerr/config
CALIBRE_CONFIG_PATH=/path/to/calibre/config
BOOKS_PATH=/path/to/books

# Timezone
TZ=UTC

# Plex Claim Token
PLEX_CLAIM=your_plex_claim_token
